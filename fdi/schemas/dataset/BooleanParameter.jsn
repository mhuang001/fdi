{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "$id": "https://fdi.schemas/dataset/BooleanParameter",
    "title": "BooleanParameter",
    "definitions": {
    },

    "description": "schema for fdi BooleanParameter type",
    "type": "object",
    "$ref": "https://fdi.schemas/Require_STID",
    "allOf": [
	{"$ref": "MetaParameter"},
	{
	    "type": "object",
	    "required": ["value"],
	    "properties": {
		"value": {
		    "description": "Truth value in true or false.",
		    "anyOf": [
			{"type": "boolean"},
			{"type": "null" }
		    ],
		    "default": null,
		    "examples": [
                        true, null
		    ]
                },
		"default": {
		    "description": "The value to assume.",
		    "anyOf": [
			{"type": "boolean"},
			{"type": "null" }
		    ],
		    "default": null,
		    "examples": [
			true, null
		    ]
		},
		"_STID": {"const": "BooleanParameter" }
	    }
	}
    ],
    "examples": [
	{"description": "foo?", "type": "boolean", "default": null,
	 "value": true, "valid": [[[false, true], "ok"]],
	 "listeners": {"_STID": "ListenerSet"},
	 "_STID": "BooleanParameter"}
    ] 
}

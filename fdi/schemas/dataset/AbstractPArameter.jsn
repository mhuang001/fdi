{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "$id": "https://fdi.schemas/dataset/AbstractParameter",
    "title": "AbstractParameter",
    "definitions": {
    },

    "type": "object",
    "$ref": "https://fdi.schemas/Require_STID",
    "allOf": [
	{"$ref": "#/definitions/MetaParameter"},
	{
	    "type": "object",
	    "required": ["value", "description"],
	    "properties": {
		"value": {
		    "description": "Amplitude or magnitude.",
		    "examples": [
			1642648857.544
		    ]
		},
		"description": {
		    "type": "string",
		    "default": "UNKNOWN",
		    "examples": [
			"Something about this parameter."
		    ]
		},
		"_STID": { "const": ["AbstractParameter"]}
	    }
	}
    ]
}

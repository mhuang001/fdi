{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "$id": "https://fdi.schemas/dataset/FineTime",
    "title": "FineTime",
    "definitions": {
	"FineTime_no_STID": {
	}
    },
    "description": "International Atomic Time",
    "type": "object",
    "required": ["tai"],
    "properties": {	
	"tai": {
	    "description": "Number of microseconds since 1958-01-01T00:00:00 GMT",
	    "type": ["integer", "null"],
	    "default": null
	},
	"format": {
	    "description": "Representation Format per Python datetime module.",
	    "type": "string",
	    "pattern": "^%.*$",
	    "default": "%Y-%m-%dT%H:%M:%S.%f",
	    "examples": ["%Y",  "%Y%m%d_%H:%M:%S" ]
	},
	"_STID": {"const": "FineTime"}
    },
    "$ref": "https://fdi.schemas/Require_STID",
    "examples": [
	    {"tai": 42424242, "format": "%Y",  "_STID": "FineTime"}
    ]
}

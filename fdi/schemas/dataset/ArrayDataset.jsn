{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "$id": "https://fdi.schemas/dataset/ArrayDataset",
    "title": "ArrayDataset",
    "definitions": {
	"ArrayDataset_no_STID": {
	    "type": "object",
	    "required": ["_ATTR_meta", "_ATTR_data"],
	    "properties": {
		"description": {"type": "string"},
		"_ATTR_meta": {"$ref": "MetaData"},
		"_ATTR_data": {
		    "anyOf": [
			{"type": "array" },
			{
			    "type": "string",
			    "contentEncoding": "quoted-printable"
			},
			{"$ref": "https://fdi.schemas/bytes"},
			{"$ref": "https://fdi.schemas/a_array"},
			{"type": "null" }
		    ]
		},
		"listeners": {"$ref": "ListenerSet"}
	    }
	}
    },
    "description": "Container for FDI ArrayDatasets.",

    "type": "object",
    "$ref": "https://fdi.schemas/Require_STID",
    "allOf": [
	{"$ref": "#/definitions/ArrayDataset_no_STID"},
	{ "properties": {"_STID": { "const": "ArrayDataset" }}}
    ]
}

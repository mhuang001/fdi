{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "$id": "https://fdi.schemas/dataset/StringParameter",
    "title": "StringParameter",
    "definitions": {
    },

    "description": "schema for fdi StringParameter type",
    "type": "object",
    "$ref": "https://fdi.schemas/Require_STID",
    "allOf": [
	{"$ref": "MetaParameter"},
	{
	    "type": "object",
	    "required": ["value"],
	    "properties": {
		"value": {
		    "description": "Contents.",
		    "anyOf": [
			{"type": "string" },
			{"type": "null" }
		    ],
		    "examples": [
                        "4", "$def", null
		    ]
                },
		"default": {
		    "description": "The value to assume.",
		    "anyOf": [
			{"type": "string" },
			{"type": "null" }
		    ],
		    "default": "",
		    "examples": [
			"42"
		    ]
		},
		"typecode": {
		    "description": "Python Typecode of the value.",
		    "anyOf": [
			{"type": "string", "pattern": "[0-9]*B"},
			{"type": "null"}
		    ],
		    "default": "B"
		},
		"_STID": {"const": "StringParameter" }
	    }
	}
    ],
    "examples": [
	{"description": "tester", "type": "string", "default": null,
	 "value": "all the fish", "valid": [[["a", "z"], "ok"]],
	 "listeners": {"_STID": "ListenerSet"},
	 "_STID": "StringParameter"}
    ]
}
